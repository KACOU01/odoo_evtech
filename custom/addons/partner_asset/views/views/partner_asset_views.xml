      <record id="partner_asset_assignment_form_view" model='ir.ui.view'>
          <field name="name">partner_asset_assignment_form_view</field>
          <field name="model">partner.asset.assignment</field>
          <field name="arch" type="xml">
              <form>
                  <header>
                      <button name="button_to_approve" string="Soumettre" type="object" class="oe_highlight" states="draft"/>
                      <button name="button_cancel" string="Annuler" type="object" class="oe_highlight" states="to_approve, done"/>
                      <button name="action_validate" string="Valider" type="object" class="oe_highlight" states="to_approve"/>
                      <button name="action_do_cancel" string="Annuler" type="object" class="oe_highlight" states="done"/>
                      <field name="state" widget="statusbar" statusbar_visible="draft,to_approve,done" statusbar_colors='{"to_approve":"blue"}'/>
                  </header>
                  <sheet>
                  <script type="text/javascript" src="./partner_asset/static/src/js/map_init_script.js"></script>
                      <div class="oe_title">
                        <label for="name" string='Reference'/>
                        <h1>
                            <field name="name" required="1" readonly='1'/>
                        </h1>
                      </div>
                      <group>
                          <group>
                              <field name="asset_id" string='Borne' required='1' attrs="{'readonly': [('state', '=', 'done')]}"/>
                          </group>
                          <group>
                              <field name="partner_id" required='1' options="{'no_create': True}" attrs="{'readonly': [('state', '=', 'done')]}"/>
                          </group>
                          <group>
                              <field name="city" string="Ville" attrs="{'readonly': [('state', '=', 'done')]}"/>
                              <field name="street" string="Rue" attrs="{'readonly': [('state', '=', 'done')]}"/>
                          </group>
<!--                          <field name="latitude" invisible="0"/>-->
<!--                        <field name="longitude" invisible="0"/>-->
<!--                        <div>-->
<!--                            Latitude: <span id="latitude_display"/>-->
<!--                        </div>-->
<!--                        <div>-->
<!--                            Longitude: <span id="longitude_display"/>-->
<!--                        </div>-->
<!--                          <iframe style="width: 100%; height: 400px; border: none;" src="./partner_asset/static/src/xml/map.html"></iframe>-->
                          <!-- <div>
                                        <iframe height="500" width="800"
                                                src="partner\static\src\xml\map.html"></iframe>
                                    </div> -->
                          
                      </group>
                  </sheet>
                  <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers" />
                    <field name="message_ids" widget="mail_thread" />
                  </div>
              </form>
          </field>
      </record>
      

